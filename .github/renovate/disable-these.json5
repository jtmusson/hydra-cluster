{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "packageRules": [
        {
            "description": ["Disable matching for helm-values to prevent duplication"],
            "matchPackageNames": ["ghcr.io/lovelaze/nebula-sync"],
            "matchManagers": ["helm-values"],
            "enabled": false
        },
        {
            "description": ["Prevent reloader upgrading above 2.0.0"],
            "matchPackageNames": ["ghcr.io/stakater/charts/reloader"],
            "allowedVersions": "<=2.0.0"
        }
    ]
}
